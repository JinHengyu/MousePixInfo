{
  "name": "electron",
  "version": "2.3.8",
  "description": "像素拾取神器,快速获得鼠标在屏幕上的像素信息,包括坐标和rgb颜色",
  "main": "main.js",
  "productName": "PixelPicker",
  "private": true,
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icon.icns --prune=true --out=output"
  },
  "dependencies": {
    "robotjs": "latest"
  },
  "devDependencies": {
    "electron": "latest"
  },
  "os": [
    "darwin"
  ]
}
